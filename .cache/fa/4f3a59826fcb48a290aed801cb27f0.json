\n      active: false\r\n    };\r\n  },\r\n  computed: {\r\n    itemStyle() {\r\n      return {\r\n        active: this.active,\r\n        disabled: this.disabled\r\n      };\r\n    }\r\n  },\r\n  inject: [\"eventBus\"],\r\n  methods: {\r\n    onClick() {\r\n      // let {width}\r\n      let y = this.$el.offsetLeft; //相对于父容器y偏移量\r\n      let { width } = this.$el.getBoundingClientRect();\r\n      width = width - 40;\r\n      this.eventBus.$emit(\"update:selected\", this.pname, width, y);\r\n    }\r\n  },\r\n  mounted() {\r\n    if (this.eventBus) {\r\n      this.eventBus.$on(\"update:selected\", vm => {\r\n        if (vm === this.pname) {\r\n          this.active = true;\r\n        } else {\r\n          this.active = false;\r\n        }\r\n      });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n$hover-active-color: #409eff;\r\n$disabled-color: #ddd;\r\n.p-tabs-pane {\r\n  & > .head {\r\n    padding: 10px 40px 10px 0;\r\n    height: 40px;\r\n    font-size: 14px;\r\n    color: #303133;\r\n    cursor: pointer;\r\n    &:hover {\r\n      color: $hover-active-color;\r\n    }\r\n  }\r\n\r\n  & > .active {\r\n    color: $hover-active-color;\r\n  }\r\n  & > .disabled {\r\n    //  cursor: not-allowed;\r\n    color: $disabled-color;\r\n    pointer-events: none;\r\n    &:hover {\r\n      color: $disabled-color;\r\n    }\r\n  }\r\n  & > .body {\r\n    padding-top: 10px;\r\n    position: absolute;\r\n    left: 0;\r\n  }\r\n}\r\n</style>\r\n\r\n\r\n"},"lineCount":null}},"error":null,"hash":"9e1969204e7dccc69a2a6b0a890742f5","cacheData":{"env":{}}}